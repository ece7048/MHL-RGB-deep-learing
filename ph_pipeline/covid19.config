[TRAIN]
    # Dataset name
    data.dataset =fewshot
    # Name of the splitting
    data.split = vinyals
    # Number of classes in train
    data.train_way = 3
    # Number of support examples
    data.train_support = 5
    # Number of query examples
    data.train_query = 5
    # Number of classes in validation
    data.test_way = 3
    # Number of support examples in validation
    data.test_support = 5
     # Number query examples in validation
    data.test_query = 5
    # Number of episodes in one epoch
    data.episodes = 100
    # Flag to use CUDA
    data.cuda = 1
    # Number of GPU if data.cuda is set to 1
    data.gpu = 0

    # Data dimenstions (width,height,channels)
    model.x_dim = 256,256,1
    # Embedding dimension
    model.z_dim = 64
    # Path to the saved model
    model.save_path = ./proto_covid.h5

    # Number of epochs to train
    train.epochs = 200
    # Name of the optimizer
    train.optim_method = Adam
    # Learning rate
    train.lr = 0.001
    # Early stopping patience
    train.patience = 100

[EVAL]

    # data
    data.dataset = fewshot
    data.split = vinyals
    data.test_way = 5
    data.test_support = 5
    data.test_query = 5
    data.episodes = 1000
    data.cuda = 1
    data.gpu = 0

    # model
    model.x_dim = 256,256,1
    model.save_path = ./proto_covid.h5
